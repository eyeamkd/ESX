{
    "manifest_version": 2,
  
    "name": "Search Shortcut ",
    "description": "Word Searcher Extension especially useful for devs",
    "version": "0.0.1",
  
    "browser_action": {
     
    }, 
    "background": {
        "scripts": ["background.js"]
    },
    
    "permissions": [
        "storage", "activeTab", "http://*/*", "https://*/*"
    ], 
      "commands": {
        "search-feature": {
          "suggested_key": {
            "default": "Ctrl+Shift+5"
          },
          "description": "Used to search keywords",
          "global": true
        },
        "stack-overflow-search": {
          "suggested_key": {
            "default": "Ctrl+Shift+4"
          },
          "description": "Used to search something directly on Stack Overflow",
          "global": true
        }, 
        "youtube-search": {
          "suggested_key": {
            "default": "Ctrl+Shift+3"
          },
          "description": "Used to search something directly on Youtube",
          "global": true
        }
      }
  }